pipeline {
    agent any
    stages {
        stage('Example Build') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Example Deploy') {
            when {
              anyOf {
                branch 'devops';branch 'master' 
                }
            }
            steps {
                echo 'Deploying'
                sh 'cat README.md'
            }
        }
    }
}
